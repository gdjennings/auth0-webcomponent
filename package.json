{
  "name": "auth0-webcomponent",
  "private": true,
  "version": "0.0.1",
  "description": "Auth0 Webcomponent",
  "files": [
    "dist/"
  ],
  "main": "dist/auth0-webcomponent.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",  
  "scripts": {
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "^0.6.14",
    "@stencil/router": "latest",
    "@types/node": "^9.4.6",
    "auth0-js": "^9.3.1",
    "moment": "^2.20.1",
    "rxjs": "^5.5.6",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.10",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdjennings/auth0-webcomponent.git"
  },
  "author": "Grant Jennings",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdjennings/auth0-webcomponent"
  },
  "homepage": "https://github.com/gdjennings/auth0-webcomponent",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
